-- Script Lua untuk Delta Executor Android
-- Update Sistem Key Notification - Fixed GUI Version

local Players = game:GetService("Players")
local TeleportService = game:GetService("TeleportService")
local StarterGui = game:GetService("StarterGui")
local TweenService = game:GetService("TweenService")

-- Fungsi untuk menampilkan notifikasi
local function showNotification()
    StarterGui:SetCore("SendNotification", {
        Title = "‚ö†Ô∏è UPDATE SISTEM KEY";
        Text = "Silakan baca pesan penting!";
        Duration = 8;
        Button1 = "OK";
    })
end

-- Fungsi untuk menampilkan GUI dengan pesan lengkap
local function createMessageGui()
    local screenGui = Instance.new("ScreenGui")
    screenGui.Name = "UpdateKeyMessage"
    screenGui.Parent = Players.LocalPlayer:WaitForChild("PlayerGui")
    
    -- Background blur overlay
    local blurFrame = Instance.new("Frame")
    blurFrame.Size = UDim2.new(1, 0, 1, 0)
    blurFrame.Position = UDim2.new(0, 0, 0, 0)
    blurFrame.BackgroundColor3 = Color3.new(0, 0, 0)
    blurFrame.BackgroundTransparency = 0.4
    blurFrame.Parent = screenGui
    
    -- Main container shadow
    local shadowFrame = Instance.new("Frame")
    shadowFrame.Size = UDim2.new(0, 408, 0, 308)
    shadowFrame.Position = UDim2.new(0.5, -204, 0.5, -154)
    shadowFrame.BackgroundColor3 = Color3.new(0, 0, 0)
    shadowFrame.BackgroundTransparency = 0.7
    shadowFrame.Parent = screenGui
    
    local shadowCorner = Instance.new("UICorner")
    shadowCorner.CornerRadius = UDim.new(0, 12)
    shadowCorner.Parent = shadowFrame
    
    -- Frame utama (hitam)
    local mainFrame = Instance.new("Frame")
    mainFrame.Size = UDim2.new(0, 400, 0, 300)
    mainFrame.Position = UDim2.new(0.5, -200, 0.5, -150)
    mainFrame.BackgroundColor3 = Color3.new(0, 0, 0) -- Hitam solid
    mainFrame.BorderSizePixel = 0
    mainFrame.Parent = screenGui
    
    -- Corner untuk frame utama
    local mainCorner = Instance.new("UICorner")
    mainCorner.CornerRadius = UDim.new(0, 8)
    mainCorner.Parent = mainFrame
    
    -- Neon Green Border Frame
    local neonFrame = Instance.new("Frame")
    neonFrame.Size = UDim2.new(1, 4, 1, 4)
    neonFrame.Position = UDim2.new(0, -2, 0, -2)
    neonFrame.BackgroundColor3 = Color3.new(0, 1, 0) -- Neon hijau
    neonFrame.Parent = mainFrame
    
    local neonCorner = Instance.new("UICorner")
    neonCorner.CornerRadius = UDim.new(0, 10)
    neonCorner.Parent = neonFrame
    
    -- Animasi glow untuk border neon
    local glowTween = TweenService:Create(
        neonFrame,
        TweenInfo.new(1.5, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, -1, true),
        {BackgroundTransparency = 0.3}
    )
    glowTween:Play()
    
    -- Header
    local headerFrame = Instance.new("Frame")
    headerFrame.Size = UDim2.new(1, 0, 0, 60)
    headerFrame.Position = UDim2.new(0, 0, 0, 0)
    headerFrame.BackgroundColor3 = Color3.new(0.05, 0.05, 0.05) -- Hitam sedikit terang
    headerFrame.BorderSizePixel = 0
    headerFrame.Parent = mainFrame
    
    local headerCorner = Instance.new("UICorner")
    headerCorner.CornerRadius = UDim.new(0, 8)
    headerCorner.Parent = headerFrame
    
    -- Icon
    local iconLabel = Instance.new("TextLabel")
    iconLabel.Size = UDim2.new(0, 40, 0, 40)
    iconLabel.Position = UDim2.new(0, 10, 0, 10)
    iconLabel.BackgroundTransparency = 1
    iconLabel.Text = "üîê"
    iconLabel.TextColor3 = Color3.new(0, 1, 0) -- Neon hijau
    iconLabel.TextScaled = true
    iconLabel.Font = Enum.Font.GothamBold
    iconLabel.Parent = headerFrame
    
    -- Title
    local titleLabel = Instance.new("TextLabel")
    titleLabel.Size = UDim2.new(0, 330, 0, 30)
    titleLabel.Position = UDim2.new(0, 60, 0, 5)
    titleLabel.BackgroundTransparency = 1
    titleLabel.Text = "SISTEM KEY UPDATE"
    titleLabel.TextColor3 = Color3.new(1, 1, 1) -- Putih
    titleLabel.TextScaled = true
    titleLabel.Font = Enum.Font.GothamBold
    titleLabel.Parent = headerFrame
    
    -- Subtitle
    local subtitleLabel = Instance.new("TextLabel")
    subtitleLabel.Size = UDim2.new(0, 330, 0, 20)
    subtitleLabel.Position = UDim2.new(0, 60, 0, 35)
    subtitleLabel.BackgroundTransparency = 1
    subtitleLabel.Text = "Authentication Required"
    subtitleLabel.TextColor3 = Color3.new(0.8, 0.8, 0.8) -- Abu-abu terang
    subtitleLabel.TextScaled = true
    titleLabel.Font = Enum.Font.Gotham
    subtitleLabel.Parent = headerFrame
    
    -- Content area
    local contentFrame = Instance.new("Frame")
    contentFrame.Size = UDim2.new(1, -20, 0, 180)
    contentFrame.Position = UDim2.new(0, 10, 0, 70)
    contentFrame.BackgroundTransparency = 1
    contentFrame.Parent = mainFrame
    
    -- Main message
    local messageLabel = Instance.new("TextLabel")
    messageLabel.Size = UDim2.new(1, 0, 0, 140)
    messageLabel.Position = UDim2.new(0, 0, 0, 0)
    messageLabel.BackgroundTransparency = 1
    messageLabel.Text = "‚ö†Ô∏è PERHATIAN PENTING ‚ö†Ô∏è\n\nSistem key telah diperbarui untuk keamanan yang lebih baik.\n\nüìû HUBUNGI WARPAH MELALUI:\n‚Ä¢ WhatsApp ‚Ä¢ Discord\n\nüìã YANG PERLU DISIAPKAN:\n‚Ä¢ Bukti transfer kemarin\n‚Ä¢ Screenshot chat sebelumnya\n\nüîë Untuk mendapatkan key baru yang valid\n\nTerima kasih atas pengertian Anda üôè"
    messageLabel.TextColor3 = Color3.new(1, 1, 1) -- Putih
    messageLabel.TextSize = 12
    messageLabel.Font = Enum.Font.Gotham
    messageLabel.TextWrapped = true
    messageLabel.TextYAlignment = Enum.TextYAlignment.Top
    messageLabel.Parent = contentFrame
    
    -- Status indicator
    local statusFrame = Instance.new("Frame")
    statusFrame.Size = UDim2.new(1, 0, 0, 25)
    statusFrame.Position = UDim2.new(0, 0, 0, 150)
    statusFrame.BackgroundColor3 = Color3.new(0.8, 0.2, 0.2) -- Merah
    statusFrame.Parent = contentFrame
    
    local statusCorner = Instance.new("UICorner")
    statusCorner.CornerRadius = UDim.new(0, 5)
    statusCorner.Parent = statusFrame
    
    local statusLabel = Instance.new("TextLabel")
    statusLabel.Size = UDim2.new(1, 0, 1, 0)
    statusLabel.Position = UDim2.new(0, 0, 0, 0)
    statusLabel.BackgroundTransparency = 1
    statusLabel.Text = "üö´ KEY TIDAK VALID - PERLU UPDATE"
    statusLabel.TextColor3 = Color3.new(1, 1, 1) -- Putih
    statusLabel.TextScaled = true
    statusLabel.Font = Enum.Font.GothamBold
    statusLabel.Parent = statusFrame
    
    -- Close Button
    local closeButton = Instance.new("TextButton")
    closeButton.Size = UDim2.new(0, 140, 0, 35)
    closeButton.Position = UDim2.new(0.5, -70, 0, 260)
    closeButton.BackgroundColor3 = Color3.new(0, 0.8, 0) -- Hijau
    closeButton.Text = "üö™ KELUAR GAME"
    closeButton.TextColor3 = Color3.new(1, 1, 1) -- Putih
    closeButton.TextScaled = true
    closeButton.Font = Enum.Font.GothamBold
    closeButton.Parent = mainFrame
    
    local closeCorner = Instance.new("UICorner")
    closeCorner.CornerRadius = UDim.new(0, 6)
    closeCorner.Parent = closeButton
    
    -- Hover effects untuk close button
    closeButton.MouseEnter:Connect(function()
        local hoverTween = TweenService:Create(
            closeButton,
            TweenInfo.new(0.2, Enum.EasingStyle.Quad),
            {Size = UDim2.new(0, 150, 0, 40), BackgroundColor3 = Color3.new(0, 1, 0)}
        )
        hoverTween:Play()
    end)
    
    closeButton.MouseLeave:Connect(function()
        local leaveTween = TweenService:Create(
            closeButton,
            TweenInfo.new(0.2, Enum.EasingStyle.Quad),
            {Size = UDim2.new(0, 140, 0, 35), BackgroundColor3 = Color3.new(0, 0.8, 0)}
        )
        leaveTween:Play()
    end)
    
    -- Pulse animation untuk status
    local pulseTween = TweenService:Create(
        statusFrame,
        TweenInfo.new(1, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, -1, true),
        {BackgroundTransparency = 0.2}
    )
    pulseTween:Play()
    
    -- Icon glow animation
    local iconGlow = TweenService:Create(
        iconLabel,
        TweenInfo.new(1.2, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, -1, true),
        {TextTransparency = 0.2}
    )
    iconGlow:Play()
    
    -- Slide in animation
    mainFrame.Position = UDim2.new(0.5, -200, 1.2, 0)
    local slideIn = TweenService:Create(
        mainFrame,
        TweenInfo.new(0.6, Enum.EasingStyle.Back, Enum.EasingDirection.Out),
        {Position = UDim2.new(0.5, -200, 0.5, -150)}
    )
    slideIn:Play()
    
    -- Fungsi untuk keluar dari game dengan animasi
    closeButton.MouseButton1Click:Connect(function()
        -- Stop semua animasi
        glowTween:Cancel()
        pulseTween:Cancel()
        iconGlow:Cancel()
        
        -- Slide out animation
        local slideOut = TweenService:Create(
            mainFrame,
            TweenInfo.new(0.4, Enum.EasingStyle.Back, Enum.EasingDirection.In),
            {Position = UDim2.new(0.5, -200, -1.2, 0)}
        )
        slideOut:Play()
        
        slideOut.Completed:Connect(function()
            screenGui:Destroy()
            Players.LocalPlayer:Kick("üîê Update sistem key diperlukan. Hubungi WARPAH untuk mendapatkan key yang valid! üîê")
        end)
    end)
    
    -- Auto close setelah 15 detik dengan countdown
    spawn(function()
        wait(10)
        if screenGui.Parent then
            -- Update status menjadi countdown
            for i = 5, 1, -1 do
                if statusLabel and statusLabel.Parent then
                    statusLabel.Text = "‚è∞ AUTO CLOSE DALAM " .. i .. " DETIK"
                    wait(1)
                end
            end
            
            if screenGui.Parent then
                local slideOut = TweenService:Create(
                    mainFrame,
                    TweenInfo.new(0.4, Enum.EasingStyle.Back, Enum.EasingDirection.In),
                    {Position = UDim2.new(0.5, -200, -1.2, 0)}
                )
                slideOut:Play()
                
                slideOut.Completed:Connect(function()
                    screenGui:Destroy()
                    Players.LocalPlayer:Kick("üîê Update sistem key diperlukan. Hubungi WARPAH untuk mendapatkan key yang valid! üîê")
                end)
            end
        end
    end)
end

-- Fungsi untuk menampilkan pesan di chat
local function sendChatMessage()
    local ReplicatedStorage = game:GetService("ReplicatedStorage")
    local chatMessage = "UPDATE SISTEM KEY BARU - CONTACT WARPAH DI WHATSAPP ATAU DISCORD üîë"
    
    -- Coba kirim ke chat jika memungkinkan
    pcall(function()
        game:GetService("StarterGui"):SetCore("ChatMakeSystemMessage", {
            Text = chatMessage;
            Color = Color3.new(0, 1, 0); -- Neon hijau
            Font = Enum.Font.SourceSansBold;
            FontSize = Enum.FontSize.Size18;
        })
    end)
end

-- Main execution
local function main()
    print("üîë DELTA EXECUTOR - UPDATE SISTEM KEY")
    print("=====================================")
    print("UPDATE SISTEM KEY BARU")
    print("CONTACT WARPAH DI WHATSAPP ATAU DISCORD")
    print("UNTUK MENGAMBIL KEY BARU DAN JUGA KIRIM")
    print("BUKTI TRANSFER KEMARIN KE CHAT WARPAH")
    print("MOHON DIMENGERTI üôè")
    print("=====================================")
    
    -- Tampilkan notifikasi
    showNotification()
    
    -- Tunggu sebentar
    wait(2)
    
    -- Kirim pesan chat
    sendChatMessage()
    
    -- Tampilkan GUI
    createMessageGui()
    
    -- Print pesan terakhir
    wait(3)
    print("‚ö†Ô∏è Script akan menutup game dalam beberapa detik...")
    print("üí¨ Hubungi WARPAH untuk mendapatkan key yang valid!")
end

-- Jalankan script
main()
