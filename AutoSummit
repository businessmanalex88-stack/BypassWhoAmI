local closeCorner = Instance.new("UICorner")
closeCorner.CornerRadius = UDim.new(0, 4)
closeCorner.Parent = closeBtn

-- Main Content Area
local contentFrame = Instance.new("Frame")
contentFrame.Name = "ContentFrame"
contentFrame.Size = UDim2.new(1, 0, 1, -35)
contentFrame.Position = UDim2.new(0, 0, 0, 35)
contentFrame.BackgroundTransparency = 1
contentFrame.Parent = mainFrame

-- Professional Auto Summit Button
local autoSummitBtn = Instance.new("TextButton")
autoSummitBtn.Name = "AutoSummitButton"
autoSummitBtn.Size = UDim2.new(0, 280, 0, 80)
autoSummitBtn.Position = UDim2.new(0.5, -140, 0.5, -40)
autoSummitBtn.BackgroundColor3 = Color3.fromRGB(0, 200, 50)
autoSummitBtn.Text = "Auto Summit"
autoSummitBtn.TextColor3 = Color3.fromRGB(0, 0, 0)
autoSummitBtn.TextSize = 20
autoSummitBtn.Font = Enum.Font.GothamBold
autoSummitBtn.BorderSizePixel = 0
autoSummitBtn.Parent = contentFrame

local autoSummitCorner = Instance.new("UICorner")
autoSummitCorner.CornerRadius = UDim.new(0, 8)
autoSummitCorner.Parent = autoSummitBtn

-- Professional gradient for main button
