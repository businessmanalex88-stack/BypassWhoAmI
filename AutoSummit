buttonGradient.Color = ColorSequence.new{
    ColorSequenceKeypoint.new(0, Color3.fromRGB(0, 220, 60)),
    ColorSequenceKeypoint.new(1, Color3.fromRGB(0, 180, 40))
}
buttonGradient.Rotation = 90
buttonGradient.Parent = autoSummitBtn

-- Summit Selection Frame
local summitFrame = Instance.new("Frame")
summitFrame.Name = "SummitFrame"
summitFrame.Size = UDim2.new(0, 300, 0, 420)
summitFrame.Position = UDim2.new(0.5, -150, 0.5, -210)
summitFrame.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
summitFrame.BorderSizePixel = 0
summitFrame.Visible = false
summitFrame.Active = true
summitFrame.Draggable = true
summitFrame.Parent = screenGui

-- Professional outline for summit frame
local summitOutline = Instance.new("UIStroke")
summitOutline.Color = Color3.fromRGB(0, 255, 65)
summitOutline.Thickness = 2
summitOutline.Parent = summitFrame

local summitCorner = Instance.new("UICorner")
summitCorner.CornerRadius = UDim.new(0, 6)
summitCorner.Parent = summitFrame

-- Summit Title Bar
local summitTitleBar = Instance.new("Frame")
summitTitleBar.Size = UDim2.new(1, 0, 0, 35)
summitTitleBar.Position = UDim2.new(0, 0, 0, 0)
summitTitleBar.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
summitTitleBar.BorderSizePixel = 0
summitTitleBar.Parent = summitFrame

local summitTitleCorner = Instance.new("UICorner")
summitTitleCorner.CornerRadius = UDim.new(0, 6)
summitTitleCorner.Parent = summitTitleBar

-- Summit title gradient
local summitTitleGradient = Instance.new("UIGradient")
summitTitleGradient.Color = ColorSequence.new{
    ColorSequenceKeypoint.new(0, Color3.fromRGB(25, 25, 25)),
    ColorSequenceKeypoint.new(1, Color3.fromRGB(40, 40, 40))
}
summitTitleGradient.Rotation = 90
summitTitleGradient.Parent = summitTitleBar

