-- Loading animation function with disclaimer
local function showLoadingAnimation(mountainName, url)
    loadingFrame.Visible = true
    loadingTitle.Text = "⏳ Loading " .. mountainName
    loadingStatus.Text = "Sedikit saran jika summit eror bisa klik manual dan usahakan ketika menginjak checkpoint tunggu sampai terhitung Stay safe buat kalian salam WarpahVIP"
    
    -- Animate progress bar
    local progressTween = TweenService:Create(progressFill, TweenInfo.new(3, Enum.EasingStyle.Quad), {Size = UDim2.new(1, 0, 1, 0)})
    progressTween:Play()
    
    -- Update after 3 seconds and execute loadstring
    spawn(function()
        wait(3)
        
        -- Execute loadstring
        if url then
            pcall(function()
                loadstring(game:HttpGet(url))()
            end)
        end
        
        loadingStatus.Text = "Script berhasil dimuat!"
        wait(0.5)
        loadingFrame.Visible = false
        
        -- Reset progress bar
        progressFill.Size = UDim2.new(0, 0, 1, 0)
    end)
end

-- Function to create mountain button
local function createMountainButton(mountain, index)
    local btnFrame = Instance.new("Frame")
    btnFrame.Size = UDim2.new(1, -5, 0, 32)
    btnFrame.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
    btnFrame.BorderSizePixel = 0
    btnFrame.Parent = scrollFrame
    
    local btnCorner = Instance.new("UICorner")
    btnCorner.CornerRadius = UDim.new(0, 4)
    btnCorner.Parent = btnFrame
    
    -- Button gradient
    local btnGradient = Instance.new("UIGradient")
    btnGradient.Color = ColorSequence.new{
        ColorSequenceKeypoint.new(0, Color3.fromRGB(45, 45, 45)),
        ColorSequenceKeypoint.new(1, Color3.fromRGB(35, 35, 35))
    }
    btnGradient.Rotation = 90
    btnGradient.Parent = btnFrame
    
    -- Icon label
    local iconLabel = Instance.new("TextLabel")
    iconLabel.Size = UDim2.new(0, 30, 1, 0)
    iconLabel.Position = UDim2.new(0, 5, 0, 0)
    iconLabel.BackgroundTransparency = 1
    iconLabel.Text = mountain.icon
    iconLabel.TextColor3 = Color3.fromRGB(0, 255, 65)
    iconLabel.TextSize = 16
    iconLabel.Font = Enum.Font.Gotham
    iconLabel.TextXAlignment = Enum.TextXAlignment.Center
    iconLabel.TextYAlignment = Enum.TextYAlignment.Center
    iconLabel.Parent = btnFrame
    
    -- Mountain name label
    local nameLabel = Instance.new("TextLabel")
    nameLabel.Size = UDim2.new(1, -40, 1, 0)
    nameLabel.Position = UDim2.new(0, 35, 0, 0)
    nameLabel.BackgroundTransparency = 1
    nameLabel.Text = mountain.name
    nameLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    nameLabel.TextSize = 14
    nameLabel.Font = Enum.Font.Gotham
    nameLabel.TextXAlignment = Enum.TextXAlignment.Left
    nameLabel.TextYAlignment = Enum.TextYAlignment.Center
    nameLabel.Parent = btnFrame
    
    -- Invisible button for click detection
    local btn = Instance.new("TextButton")
    btn.Size = UDim2.new(1, 0, 1, 0)
    btn.BackgroundTransparency = 1
    btn.Text = ""
    btn.Parent = btnFrame
    
    -- Hover effects
    btn.MouseEnter:Connect(function()
        btnGradient.Color = ColorSequence.new{
            ColorSequenceKeypoint.new(0, Color3.fromRGB(0, 180, 40)),
            ColorSequenceKeypoint.new(1, Color3.fromRGB(0, 140, 30))
        }
        nameLabel.TextColor3 = Color3.fromRGB(0, 0, 0)
    end)
    
    btn.MouseLeave:Connect(function()
        btnGradient.Color = ColorSequence.new{
            ColorSequenceKeypoint.new(0, Color3.fromRGB(45, 45, 45)),
            ColorSequenceKeypoint.new(1, Color3.fromRGB(35, 35, 35))
        }
        nameLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    end)
    
    -- Button click function
    btn.MouseButton1Click:Connect(function()
        if mountain.url then
            -- Hide summit frame immediately
            summitFrame.Visible = false
            
            showLoadingAnimation(mountain.name, mountain.url)
            
            pcall(function()
                game:GetService("StarterGui"):SetCore("SendNotification", {
                    Title = "✅ " .. mountain.name,
                    Text = mountain.name .. " script loading...",
                    Duration = 3
                })
            end)
        else
            pcall(function()
                game:GetService("StarterGui"):SetCore("SendNotification", {
                    Title = "⚠️ " .. mountain.name,
                    Text = "Script belum tersedia",
                    Duration = 3
                })
            end)
            summitFrame.Visible = false
        end
        
        print("🎯 " .. mountain.name .. " function executed")
    end)
end

-- Create all mountain buttons
for i, mountain in ipairs(mountains) do
    createMountainButton(mountain, i)
end

-- Set scroll canvas size
scrollFrame.CanvasSize = UDim2.new(0, 0, 0, (#mountains * 35) + 10)

-- Button functions
local isMinimized = false

minimizeBtn.MouseButton1Click:Connect(function()
    if not isMinimized then
        local tween = TweenService:Create(mainFrame, TweenInfo.new(0.2, Enum.EasingStyle.Quad), {Size = UDim2.new(0, 320, 0, 35)})
        tween:Play()
        contentFrame.Visible = false
        isMinimized = true
    else
        local tween = TweenService:Create(mainFrame, TweenInfo.new(0.2, Enum.EasingStyle.Quad), {Size = UDim2.new(0, 320, 0, 180)})
        tween:Play()
        contentFrame.Visible = true
        isMinimized = false
    end
end)

closeBtn.MouseButton1Click:Connect(function()
    screenGui:Destroy()
end)

autoSummitBtn.MouseButton1Click:Connect(function()
    summitFrame.Visible = true
end)

closeSummitBtn.MouseButton1Click:Connect(function()
    summitFrame.Visible = false
end)

-- OK button for disclaimer
okButton.MouseButton1Click:Connect(function()
    disclaimerFrame.Visible = false
end)

-- Hover effects
autoSummitBtn.MouseEnter:Connect(function()
    buttonGradient.Color = ColorSequence.new{
        ColorSequenceKeypoint.new(0, Color3.fromRGB(0, 240, 70)),
        ColorSequenceKeypoint.new(1, Color3.fromRGB(0, 200, 50))
    }
end)

autoSummitBtn.MouseLeave:Connect(function()
    buttonGradient.Color = ColorSequence.new{
        ColorSequenceKeypoint.new(0, Color3.fromRGB(0, 220, 60)),
        ColorSequenceKeypoint.new(1, Color3.fromRGB(0, 180, 40))
    }
end)

minimizeBtn.MouseEnter:Connect(function()
    minimizeBtn.BackgroundColor3 = Color3.fromRGB(80, 80, 80)
end)

minimizeBtn.MouseLeave:Connect(function()
    minimizeBtn.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
end)

closeBtn.MouseEnter:Connect(function()
    closeBtn.BackgroundColor3 = Color3.fromRGB(240, 70, 70)
end)

closeBtn.MouseLeave:Connect(function()
    closeBtn.BackgroundColor3 = Color3.fromRGB(220, 50, 50)
end)

closeSummitBtn.MouseEnter:Connect(function()
    closeSummitBtn.BackgroundColor3 = Color3.fromRGB(240, 70, 70)
end)

closeSummitBtn.MouseLeave:Connect(function()
    closeSummitBtn.BackgroundColor3 = Color3.fromRGB(220, 50, 50)
end)

okButton.MouseEnter:Connect(function()
    okButton.BackgroundColor3 = Color3.fromRGB(0, 220, 70)
end)

okButton.MouseLeave:Connect(function()
    okButton.BackgroundColor3 = Color3.fromRGB(0, 200, 50)
end)

