-- ðŸ’¥ FIXED: Pembersih GUI executor (tidak hapus analog/loncat)

local player = game:GetService("Players").LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")

-- Daftar GUI penting Roblox yang tidak boleh dihapus
local protectedGuiNames = {
	"RobloxGui",
	"Chat",
	"TouchGui",      -- GUI kontrol analog
	"PlayerList",    -- Daftar pemain
	"BubbleChat",
	"Backpack"       -- GUI inventory jika aktif
}

-- Fungsi cek apakah GUI ini berasal dari executor (bukan dari StarterGui dan bukan sistem Roblox)
local function isExecutorGui(gui)
	if not gui:IsA("ScreenGui") then return false end
	if game.StarterGui:FindFirstChild(gui.Name) then return false end
	for _, protected in ipairs(protectedGuiNames) do
		if gui.Name == protected then
			return false
		end
	end
	return true
end

-- Hapus semua GUI dari executor
for _, gui in ipairs(playerGui:GetChildren()) do
	if isExecutorGui(gui) then
		warn("[Delta GUI Cleaner] Menghapus GUI dari executor:", gui.Name)
		pcall(function()
			gui:Destroy()
		end)
	end
end

print("[Delta GUI Cleaner] GUI executor dibersihkan tanpa ganggu kontrol Roblox âœ…")
