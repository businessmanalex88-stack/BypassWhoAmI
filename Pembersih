-- ðŸ’¥ Pembersih GUI dari Executor Delta, tanpa crash/kick

local player = game:GetService("Players").LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")

-- Fungsi untuk mengecek apakah GUI berasal dari executor (bukan dari StarterGui)
local function isExecutorGui(gui)
	return gui:IsA("ScreenGui") and not game.StarterGui:FindFirstChild(gui.Name)
end

-- Hapus semua GUI yang di-load oleh executor (tidak berasal dari game)
for _, gui in pairs(playerGui:GetChildren()) do
	if isExecutorGui(gui) then
		print("[GUI Cleaner] Menghapus GUI executor:", gui.Name)
		pcall(function()
			gui:Destroy()
		end)
	end
end

print("[GUI Cleaner] Semua GUI dari executor telah dibersihkan âœ…")
