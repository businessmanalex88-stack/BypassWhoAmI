-- ðŸ’¥ Executor Delta GUI Cleaner (Tidak crash & hanya hapus GUI dari executor)

local player = game:GetService("Players").LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")

-- Cek apakah GUI berasal dari executor (bukan dari StarterGui dan bukan GUI sistem bawaan Roblox)
local function isExecutorGui(gui)
	if not gui:IsA("ScreenGui") then return false end
	if game.StarterGui:FindFirstChild(gui.Name) then return false end
	if gui.Name == "RobloxGui" or gui.Name == "Chat" then return false end -- GUI bawaan
	if gui:FindFirstChildOfClass("TextButton") == nil and gui:FindFirstChildOfClass("Frame") == nil then
		return false -- Bukan GUI dengan tombol/konten yang bisa berinteraksi
	end
	return true
end

-- Hapus semua GUI dari executor
for _, gui in ipairs(playerGui:GetChildren()) do
	if isExecutorGui(gui) then
		warn("[Delta GUI Cleaner] Menghapus GUI dari executor:", gui.Name)
		pcall(function()
			gui:Destroy()
		end)
	end
end

print("[Delta GUI Cleaner] Selesai menghapus semua GUI executor dengan aman âœ…")
